{
  "presets": [
    "es2015",
    "react",
    "stage-2"
  ],
  "env": {
    "development": {
      "plugins": [
        "dva-hmr",
        "transform-runtime",
        "add-module-exports",
        [
          "import",
          [
            {
              "libraryName": "antd",
              "style": "css"
            }
          ]
        ]
      ]

    },
    "production": {
      "plugins": [
        "transform-runtime",
        "add-module-exports",
        [
          "import",
          [
            {
              "libraryName": "antd",
              "style": "css"
            }
          ]
        ]
      ]

    }

  }
}
